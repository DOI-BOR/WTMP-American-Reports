					<Parameter>Temperature</Parameter>
					<XLabel>Temperature (%%units%%)</XLabel>
					<SplitByYear>True</SplitByYear>										
					<profilesperrow>4</profilesperrow>
					<Lines>
						<Line>
							<label>Observed</label>
							<Flag>Observed</Flag>
							<Parameter>Temperature</Parameter>
							<lineColor>#5b9bd5</lineColor>			<!--medium blue-->
							<lineWidth>2</lineWidth>
							<lineStylePattern>solid</lineStylePattern>
							<zorder>4</zorder>
							<FileName>%%observedDir%%\LakeNatomaTempProfiles_ordered_ft.txt</FileName>
							<y_convention>depth</y_convention>
							<units>C</units>						<!--Text file input, so specify units-->							
							<wse>
								<DSS_Filename>%%observedDir%%/Natoma_data_6.dss</DSS_Filename>
								<parameter>elevation</parameter>
								<DSS_Path>/NATOMA/NATOMA POOL/ELEV/*/1HOUR/CDEC-CARDNO/</DSS_Path>
							</wse>
						</Line>					
						<Line>
							<label>Simulated</label>
							<Flag>Computed</Flag>
							<Parameter>Temperature</Parameter>
							<linecolor>red</linecolor>								
							<lineWidth>2</lineWidth>
							<lineStylePattern>dashed</lineStylePattern>
							<zorder>3</zorder>							
							<w2_segment>34</w2_segment>						
							<omitvalue>-146.2</omitvalue>		<!--Because simulated may have null values, F equivaluent of -99C-->
						</Line>
					</Lines>
