<Chapters>
	<Chapter>
		<Name>Iterations</Name>
		<debug>true</debug>
		<memberiteration>true</memberiteration>
		<Sections>

			<Section>
				<header>Hazel Ave Water Temperature Iteration %%member%%</header>
				<Object>
					<type>timeseriesplot</type>
					<title>Hazel Ave Temperature, W2 Regression and ResSim Modeled: Iteration %%member%%</title>
					<Description>Hazel Ave temperature comparison %%year%% Iteration %%member%%</Description>
					<ylabel>Water Temperature (%%units%%)</ylabel>
					<unitsystem>english</unitsystem>
					<legend_outside>True</legend_outside>
					<lines>
						<line>
							<flag>W2 Hazel Regression</flag>
							<label>W2 Hazel Regression</label>
							<DSS_Filename>%%SimulationDir%%\forecastResults.dss</DSS_Filename>
							<DSS_Path>//W2_DOWNSTREAMREGRESSIONHAZEL/TEMP-WATER//1DAY/*|%%FPART%%/</DSS_Path>
							<interval>1DAY</interval>
						</line>
						<line>
							<flag>ResSim Hazel Ave Modeled</flag>
							<label>ResSim Hazel Ave Modeled</label>
							<DSS_Filename>%%SimulationDir%%\forecastResults.dss</DSS_Filename>
							<DSS_Path>//AFO/TEMP-WATER TEMPERATURE//1HOUR/*|%%FPART%%/</DSS_Path>
							<interval>1DAY</interval>
						</line>
					</lines>
				</Object>
				<Object>
					<type>ErrorStatisticsTable</type>
					<primarykeyheader>Location</primarykeyheader>
					<unitsystem>english</unitsystem>
					<description>Iteration %%member%% Statistical Summary May-Sep %%year%% (%%units%%)</description>
					<xlims>
						<min>01 May %%year%%</min>
						<max>01 Sep %%year%%</max>
					</xlims>
					<Datapaths>
						<Datapath>
							<flag>Regression</flag>
							<DSS_Filename>%%SimulationDir%%\forecastResults.dss</DSS_Filename>
							<DSS_Path>//W2_DOWNSTREAMREGRESSIONHAZEL/TEMP-WATER//1DAY/*|%%FPART%%/</DSS_Path>
							<interval>1DAY</interval>
						</Datapath>
						<Datapath>
							<flag>ResSim</flag>
							<DSS_Filename>%%SimulationDir%%\forecastResults.dss</DSS_Filename>
							<DSS_Path>//AFO/TEMP-WATER TEMPERATURE//1HOUR/*|%%FPART%%/</DSS_Path>
							<interval>1DAY</interval>
						</Datapath>
					</Datapaths>
					<rows>
						<row>Regression|%%maximum.Regression%%|%%mean.Regression%%|%%minimum.Regression%%</row>
						<row>ResSim|%%maximum.ResSim%%|%%mean.ResSim%%|%%minimum.ResSim%%</row>
					</rows>
					<headers>
						<header>Maximum</header>
						<header>Mean</header>
						<header>Minimum</header>
					</headers>
				</Object>
				
				
