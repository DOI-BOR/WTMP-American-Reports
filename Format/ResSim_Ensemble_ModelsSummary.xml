							<!-- <filename>%%SimulationDir%%\SRTTG_reporting.csv</filename> -->
						<!-- </Datapath> -->
					<!-- </Datapaths> -->
				<!-- </Object> -->
				<Object>
					<type>ErrorStatisticsTable</type>
					<primarykeyheader>Iteration</primarykeyheader>
					<unitsystem>english</unitsystem>
					<description>Folsom Lake Outflow Iteration Statistical Summary May-Sep %%year%% (%%units%%)</description>
					<xlims>
						<min>01 May %%year%%</min>
						<max>01 Sep %%year%%</max>
					</xlims>
					<Datapaths>
						<Datapath>
							<flag>Computed</flag>
							<label>Computed</label>
							<DSS_Filename>%%SimulationDir%%\forecastResults.dss</DSS_Filename>
							<DSS_Path>//FOLSOM DAM/TEMP-WATER TEMPERATURE//1HOUR/*|%%FPART%%/</DSS_Path>
							<interval>1DAY</interval>
						</Datapath>
					</Datapaths>
					<rows>
						<row>%%member%%|%%maximum.Computed%%|%%mean.Computed%%|%%minimum.Computed%%</row>
					</rows>
					<headers>
						<header>Maximum</header>
						<header>Mean</header>
						<header>Minimum</header>
					</headers>
				</Object>
			</Section>
