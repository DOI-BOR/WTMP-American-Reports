<Chapters>
	<Chapter>
		<Name>Summary of Models</Name>
		<debug>true</debug>
		<Sections>
			<Section>
				<header>Iteration Summary</header>
				<Object>
					<type>forecasttable</type>
					<Description>Iteration run information</Description>
				</Object>
			</Section>
			<Section>
				<header>Folsom</header>
				<Object>
					<type>timeseriesplot</type>
					<title>Folsom Lake Outflow Forecast Runs with Envelopes</title>
					<Description>Folsom Lake Outflow Temperature variable input with percentile envelopes computed</Description>
					<ylabel>Water Temperature (%%units%%)</ylabel>
					<unitsystem>english</unitsystem>
					<legend_outside>True</legend_outside>
					<lines>
						<line>
							<flag>computed</flag>
							<label>Computed</label>
							<DSS_Filename>%%SimulationDir%%\forecastResults.dss</DSS_Filename>
							<DSS_Path>//FOLSOM DAM/TEMP-WATER TEMPERATURE//1HOUR/*|%%FPART%%/</DSS_Path>
							<linecolor>darkgreen</linecolor>
							<interval>1DAY</interval>
							<envelopes>
								<envelope>
									<percent>100</percent>
									<label>Maximum</label>
									<linestylepattern>dotted</linestylepattern>
									<linewidth>3</linewidth>
								</envelope>
								<envelope>
									<percent>0</percent>
									<label>Minimum</label>
									<linewidth>3</linewidth>
									<linestylepattern>dotted</linestylepattern>
								</envelope>
								<envelope>
									<percent>25</percent>
									<label>Percentile: 25%</label>
									<linestylepattern>dashed</linestylepattern>
								</envelope>
								<envelope>
									<percent>75</percent>
									<label>Percentile 75%</label>
									<linestylepattern>dashed</linestylepattern>
								</envelope>
								<envelope>
									<percent>50</percent>
									<label>Mean</label>
									<linewidth>3</linewidth>
									<linestylepattern>solid</linestylepattern>
								</envelope>
							</envelopes>
						</line>
					</lines>
				</Object>
				<Object>
					<type>timeseriesplot</type>
					<title>Folsom Lake Outflow Forecast Runs</title>
					<Description>Folsom Lake Outflow Temperature variable input</Description>
					<ylabel>Water Temperature (%%units%%)</ylabel>
					<legend_outside>True</legend_outside>
					<unitsystem>english</unitsystem>
					<lines>
						<line>
							<flag>computed</flag>
							<label>%%member%%</label>
							<DSS_Filename>%%SimulationDir%%\forecastResults.dss</DSS_Filename>
							<DSS_Path>//FOLSOM DAM/TEMP-WATER TEMPERATURE//1HOUR/*|%%FPART%%/</DSS_Path>
							<interval>1DAY</interval>
							<Type>PER-AVER</Type>
							<coloreach>true</coloreach>
						</line>
					</lines>
				</Object>
				<!-- <Object> -->
					<!-- <Type>tablefromfile</Type> -->
					<!-- <Description>Folsom Lake Iterations</Description> -->
